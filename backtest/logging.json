{
  "version": 1,
  "disable_existing_loggers": true,
  "formatters": {
    "default": {
      "format": "%(asctime)s %(filename)-11s %(levelname)-8s %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "default"
    }
  },
  "loggers": {
    "__main__.py": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "router.py": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "fetcher.py": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "analyzer.py": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "broker.py": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    },
    "ledger.py": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ]
    }
  }
}
